<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css"
          integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous">
    <!-- Custom Stylesheets -->
    <link rel="stylesheet" href="../assets/css/styles.css">
    <!--Font Awesome-->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp" crossorigin="anonymous">

    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js" integrity="sha384-smHYKdLADwkXOn1EmN1qk/HfnUcbVRZyYmZ4qpPea6sjB/pTJ0euyQp0Mk8ck+5T" crossorigin="anonymous"></script>

    <!-- Editor -->
    <script src="//cdn.ckeditor.com/4.9.2/full/ckeditor.js"></script>
    <script>
        CKEDITOR.replace( 'editor1' );
    </script>
</head>

<title><%= title %></title>

<% include partials/navbar.ejs %>


<!-- Services Section -->
<div class="services px-3 py-3 pt-md-5 pb-md-4 mt-3 mx-auto text-center">
    <h1 class="display-4"><%= pageHeader %></h1>
    <p class="lead"><%= pageLead %></p>
</div>
<section class="services">
    <div class="container">
        <div class="card-deck mb-3 text-center">
            <div class="card mb-4 box-shadow">
                <div class="card-header">
                    <h4 class="my-0 font-weight-normal"><%= ocMessages.pages.servicePage.translation_2 %></h4>
                </div>
                <div class="card-body">
                    <h3 class="card-title pricing-card-title"><%= ocMessages.pages.servicePage.translation_3 %>
                        <br>
                        <small class="text-muted"><%= ocMessages.pages.servicePage.translation_4 %></small>
                    </h3>
                    <ul class="list-unstyled mt-3 mb-4">
                        <% for(var i = 0; i < cardBodyOne.length; i++){ %>
                        <li><%= cardBodyOne[i] %></li>
                        <% } %>
                    </ul>
                    <button href="#" type="button" class="btn btn-lg btn-block btn-outline-success" data-toggle="modal" data-target="#designModal"><%= ocMessages.buttons.translation_2 %></button>
                </div>
            </div>
            <div class="card mb-4 box-shadow">
                <div class="card-header">
                    <h4 class="my-0 font-weight-normal"><%= ocMessages.pages.servicePage.translation_10 %></h4>
                </div>
                <div class="card-body">
                    <h3 class="card-title pricing-card-title"><%= ocMessages.pages.servicePage.translation_17 %>
                        <small class="text-muted"><%= ocMessages.pages.servicePage.translation_18 %></small>
                    </h3>
                    <ul class="list-unstyled mt-3 mb-4">
                        <% for(var i = 0; i < cardBodyTwo.length; i++){ %>
                        <li><%= cardBodyTwo[i] %></li>
                        <% } %>
                    </ul>
                    <button href="#" type="button" class="btn btn-lg btn-block btn-outline-success mt-5" data-toggle="modal" data-target="#siteModal"><%= ocMessages.buttons.translation_2 %></button>
                </div>
            </div>
            <div class="card mb-4 box-shadow">
                <div class="card-header">
                    <h4 class="my-0 font-weight-normal"><%= ocMessages.pages.servicePage.translation_11 %></h4>
                </div>
                <div class="card-body">
                    <h4 class="card-title pricing-card-title"><%= ocMessages.pages.servicePage.translation_12 %></h4>
                    <ul class="list-unstyled mt-3 mb-4">
                        <% for(var i = 0; i < cardBodyThree.length; i++){ %>
                        <li><%= cardBodyThree[i] %></li>
                        <% } %>
                    </ul>
                    <button href="#" type="button" class="btn btn-lg btn-block btn-outline-success" data-toggle="modal" data-target="#maintenanceModal"><%= ocMessages.buttons.translation_2 %></button>
                </div>
            </div>
        </div>
    </div>
</section>
<% include partials/footer.ejs %>

<div class="modal fade" id="designModal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header bg-primary text-white">
                <h5 class="modal-title"><%= ocMessages.modals.servicePageModals.translation_0 %></h5>
            </div>
            <div class="modal-body">
                <div class="container">
                    <p class="lead"><%=ocMessages.modals.servicePageModals.translation_1 %></p>
                    <p>Delete this and fill out this modal</p>
                </div>
            </div>
            <div class="modal-footer">
                <button class="btn btn-secondary" data-dismiss="modal"><%= ocMessages.buttons.translation_0 %></button>
                <button class="btn btn-success" data-dismiss="modal"><%= ocMessages.buttons.translation_1 %></button>
            </div>
        </div>
    </div>
</div>