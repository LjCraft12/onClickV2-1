<%  include partials/navbar.ejs %>
<title>QUESTIONNAIRE</title>

<body>

<form>
<div class="container questionnaireForm">

    <!--questionnaire 1 -->
    <div class="form-group questionnaire1">
        <label for="formGroupExampleInput" class="animated fadeIn"><%= ocMessages.pages.questionnaire.translation_0 %></label>
        <input type="text" class="form-control" name="question1" id="formGroupExampleInput" placeholder="">
        <label for="formGroupExampleInput" class="animated fadeIn"><%= ocMessages.pages.questionnaire.translation_1 %></label>
        <input type="text" class="form-control" name="question2" id="formGroupExampleInput" placeholder="">
    </div>

    <!--questionnaire 2 -->
    <div class="form-group questionnaire2">
        <div class="form-group">
            <label for="exampleFormControlTextarea1" class="animated fadeIn"><%= ocMessages.pages.questionnaire.translation_2 %></label>
            <textarea class="form-control" name="questionnaire2" name="question3" id="exampleFormControlTextarea1" rows="3"></textarea>
        </div>
    </div>

    <!--questionnaire 3 -->
    <div class="form-group questionnaire3">
        <div class="form-group">
            <label for="exampleFormControlTextarea1" class="animated fadeIn"><%= ocMessages.pages.questionnaire.translation_3 %></label>
            <textarea class="form-control" name="question4" id="exampleFormControlTextarea1" rows="3"></textarea>
        </div>
    </div>

    <!--questionnaire 4 -->
    <div class="form-row align-items-center">
        <div class="col-auto my-1">
            <label class="mr-sm-2" for="inlineFormCustomSelect"><%= ocMessages.pages.questionnaire.translation_4 %></label>
            <select class="custom-select mr-sm-2" name="question5" id="inlineFormCustomSelect">
                <option selected>Choose...</option>
                <option value="1">Yes</option>
                <option value="2">No</option>
            </select>

            <label class="mr-sm-2" for="inlineFormCustomSelect"><%= ocMessages.pages.questionnaire.translation_5 %></label>
            <select class="custom-select mr-sm-2" name="question6" id="inlineFormCustomSelect">
                <option selected>Choose...</option>
                <option value="1">Yes</option>
                <option value="2">No</option>
            </select>
        </div>
    </div>

<!-- HTML Form (wrapped in a .bootstrap-iso div) -->
<div class="bootstrap-iso">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-6 col-sm-6 col-xs-12">
                <form method="post">
                    <div class="form-group">
                        <label class="control-label " for="date"><%= ocMessages.pages.questionnaire.translation_6 %>
                        </label>
                        <input class="form-control" id="date" name="date" placeholder="MM/DD/YYYY" type="text"/>
                    </div>
                    <div class="form-group">
                        <div>
                            <button class="btn btn-success btn-lg pull-right" name="submit" type="submit">
                                Send!
                            </button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

</div>

</form>

<%  include partials/footer.ejs %>
<!-- Extra JavaScript/CSS added manually in "Settings" tab -->
<!-- Include jQuery -->

<script>
    $(document).ready(function(){
        var date_input=$('input[name="date"]'); //our date input has the name "date"
        var container=$('.bootstrap-iso form').length>0 ? $('.bootstrap-iso form').parent() : "body";
        date_input.datepicker({
            format: 'mm/dd/yyyy',
            container: container,
            todayHighlight: true,
            autoclose: true,
        })
    })
</script>


</body>
</html>