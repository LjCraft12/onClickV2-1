<div class="modal fade mt-5" id="perryModal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="container">
                <header id="main-header">
                    <div class="rox no-gutters">
                        <div class="col-lg-4 col-md-5">
                            <img src="assets/images/pcra.png" alt="">
                        </div>
                        <div class="col-lg-8 col-md-7">
                            <div class="d-flex flex-column">
                                <div class="p-2 bg-dark text-white"></div>
                                <div class="name d-flex flex-row justify-content-between align-items-center">
                                    <h1 class="display-5">
                                        <%= ocMessages.ocPeople.perry.translation_0 %>
                                    </h1>
                                    <i class="fab fa-facebook-square"></i>
                                    <i class="fab fa-instagram"></i>
                                    <i class="fab fa-github"></i>
                                </div>
                            </div>
                            <div class="p-2 bg-black text-white">
                                <%= ocMessages.ocPeople.perry.translation_1 %>
                            </div>
                            <div>
                                <div class="d-flex flex-row text-white align-items-stretch text-center">
                                    <div class="port-item bg-primary">
                                        <i class="fas fa-home d-block"></i> Home
                                    </div>
                                    <div class="port-item bg-warning">
                                        <i class="fas fa-paperclip"></i> Resume
                                    </div>
                                    <div class="port-item bg-danger">
                                        <i class="fab fa-github-square"></i> Github
                                    </div>

                                    <div class="port-item bg-info">
                                        <i class="fas fa-address-card"></i> Contact
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </header>
            </div>
            <div class="modal-body">

            </div>
            <div class="modal-footer">
            </div>
        </div>
    </div>
</div>

