<title> <%= title %> </title>
<div id="main-header" class="py-2 bg-success text-white">
    <div class="container">
        <div class="row">
            <div class="col-md6">
                <h1><%= pageTitle %></h1>
            </div>
        </div>
    </div>
</div>
<div class="container">
    <form action="/admin" method="POST" id="loginPage" class="form">

        <div class="form-group  contact">
            <label for=""><%= adminUsername %></label>
            <input type="email" class="form-control mb-2" placeholder="Username" name="adminLoginUsername">
        </div>

        <div class="form-group  contact">
            <label for=""><%= adminPassword %></label>
            <input type="text" class="form-control mb-2" placeholder="Password" name="adminLoginPassword">
        </div>

        </br>

        <div class="form-group">
            <button class="btn btn-primary" type="submit">Login</button>
        </div>

    </form>
</div>